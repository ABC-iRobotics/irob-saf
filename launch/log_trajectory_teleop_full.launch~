<!-- Launch file to log a trajectory during teleoperation -->

<launch>
  <arg name="arm1" default="PSM1"/>
  <arg name="arm2" default="PSM2"/>
  <arg name="rate" default="200"/>
  <arg name="filename" default="/home/tamas/catkin_ws/data/dvrk_tr_log.dat"/>
  
  <node name="log_trajectory_1"
  	pkg="irob_dvrk_automation" type="log_trajectory" output="screen">
  		
  		<param name="arm" type="string" value="$(arg arm1)" />
  		<param name="rate" type="double" value="$(arg rate)" />
  		<param name="filename" type="string" value="$(arg filename)_$(arg arm1)" />

  </node>
  
  <node name="log_trajectory_2"
  	pkg="irob_dvrk_automation" type="log_trajectory" output="screen">
  		
  		<param name="arm" type="string" value="$(arg arm2)" />
  		<param name="rate" type="double" value="$(arg rate)" />
  		<param name="filename" type="string" value="$(arg filename)_$(arg arm2)" />

  </node>
  
</launch>
