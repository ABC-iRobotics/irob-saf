<!-- Launch file to move on a preprogrammed trajectory -->

<launch>
	<group ns="irob_autosurg">
	
	<include 
		file="$(find irob_autosurg)/config/robot_interface_topic_names.xml" />

  	<arg name="arm_name" default="arm_1"/>   
  	<arg name="rate" default="100"/>
  	<arg name="speed" default="1"/>

  
	<node name="move_circles_action_client"
  		pkg="irob_autosurg" type="move_circles_action_client" output="screen">

  		
  		<param name="arm_name" type="string" value="$(arg arm_name)" />
  		<param name="rate" type="double" value="$(arg rate)" />
  		<param name="speed" type="double" value="$(arg speed)" />

	</node>
  	</group>
</launch>
