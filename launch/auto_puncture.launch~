<!-- Launch file to move on a preprogrammed trajectory -->

<launch>

  <arg name="arm" default="PSM2"/>
  <arg name="rate" default="100"/>
  <arg name="filename_base"
  	default="/home/dvrk_nat/catkin_ws/data/puncture/puncture_test"/>
  	
  <arg name="area_x" default="-250"/>
  <arg name="area_y" default="-50"/>
  <arg name="nlocs_x" default="9"/>
  <arg name="nlocs_y" default="1"/>
  <arg name="ntrials" default="1"/>
  <arg name="depth" default="2.0"/>
  <arg name="speed" default="1.0"/>
  <arg name="T" default="5.0"/>
  
  <node name="dvrk_auto_puncture_test"
  	pkg="irob-dvrk-automation" type="dvrk_auto_puncture_test" output="screen" 
  	args="$(arg arm) $(arg rate) $(arg filename_base) $(arg area_x) $(arg area_y) $(arg nlocs_x) $(arg nlocs_y) $(arg ntrials) $(arg depth) $(arg speed) $(arg T)"/>
  	
</launch>
