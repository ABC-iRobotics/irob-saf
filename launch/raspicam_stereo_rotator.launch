<!-- Launch file for left and right camera nodes using cv-camera, also rotates the camera streams -->

<launch>
	<group ns="ias"><group ns="stereo">
		<include 
			file="$(find irob_autosurg)/launch/camera/republish_stereo_raw.launch">
			<arg name="left_in" 
					value="/ias/stereo/left/raspicam/image"/> 
			<arg name="right_in" 
					value="/ias/stereo/right/raspicam/image"/> 
		
			<arg name="left_out" 
					value="/ias/stereo/left/raspicam/image/image_raw"/> 
			<arg name="right_out" 
					value="/ias/stereo/right/raspicam/image/image_raw"/> 
		</include> 
		
		<include 
			file="$(find irob_autosurg)/launch/camera/rotate_stereo_imgs.launch">
			
			<arg name="left_angle" value="-90"/> 
			<arg name="left_in" value="left/raspicam/image/image_raw" />
			<arg name="right_in" value="right/raspicam/image/image_raw" />
			
			<arg name="right_angle" value="90"/> 
			<arg name="left_out" value="left/final/image" />
			<arg name="right_out" value="right/final/image" />
		</include> 
		
  	</group>
  	</group>
</launch>
