<!-- Launch file to log a preprogrammed trajectory -->

<launch>

  	<arg name="arm" default="PSM1"/>
  	<arg name="filename" default="/home/tamas/catkin_ws/data/dvrk_tr_log.dat"/>
  	<arg name="rate" default="100"/>
  	<arg name="speed" default="1"/>
  
  	<node name="move_circles"
  		pkg="irob-dvrk-automation" type="move_circles" output="screen">
  		
  		<param name="arm" type="string" value="$(arg arm)" />
  		<param name="rate" type="double" value="$(arg rate)" />
  		<param name="speed" type="double" value="$(arg speed)" />

	</node>
  
  	<node name="log_trajectory"
  	pkg="irob-dvrk-automation" type="log_trajectory" output="screen">
  		
  		<param name="arm" type="string" value="$(arg arm)" />
  		<param name="rate" type="double" value="$(arg rate)" />
  		<param name="filename" type="string" value="$(arg filename)" />

	</node>
  	
</launch>
