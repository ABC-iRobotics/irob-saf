<!-- Launch file to move on a preprogrammed trajectory -->

<launch>

  	<arg name="arm" default="PSM1"/>
  	<arg name="rate" default="100"/>
  	<arg name="speed" default="1"/>

  
	<node name="move_circles"
  		pkg="irob_dvrk_automation" type="move_circles" output="screen">
  		
  		<param name="arm" type="string" value="$(arg arm)" />
  		<param name="rate" type="double" value="$(arg rate)" />
  		<param name="speed" type="double" value="$(arg speed)" />

	</node>
  	
</launch>
