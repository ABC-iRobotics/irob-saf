<!-- Launch file to play a logged trajectory -->

<launch>
  <arg name="arm" default="PSM1"/>
  <arg name="init_needed" default="no_init_pos"/>
  <arg name="filename" default="/home/tamas/catkin_ws/data/dvrk_tr_log.dat"/>
  <arg name="speed" default="1.0"/>
  
  <node name="dvrk_play_logged_trajectory"
  	pkg="irob-dvrk-automation" type="dvrk_play_logged_trajectory" output="screen" 
  	args="$(arg arm) $(arg init_needed) $(arg filename) $(arg speed)"/>
  	
</launch>
