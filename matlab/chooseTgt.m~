function [ minIdx ] = chooseTgt(cuttingXYZ, groupN, lastIdxs)

%groupN = 5;

dataZ = double(zeros((idivide(uint32(size(cuttingXYZ,1)), groupN, 'floor')), groupN));
 for i = 1:(groupN*idivide(size(cuttingXYZ,1), uint32(groupN), 'floor'))
     dataZ((idivide(i-1, uint32(groupN), 'floor'))+1, mod(i-1,groupN)+1) = cuttingXYZ(i,3);
%      disp(i)
%      disp(dataZ)
 end
 
 avgDataZ = mean(dataZ, 2);
 
 [B, I] = sort(avgDataZ);
 minIdx = 0;
 i = 0;
 while minIdx == 0
     if not(any(I(i) == lastIdxs))
       minIdx = I(i);
 else
       minIdx = I(2);
 end
     
     
 end
 for i = 1:size
 if not(I(1) == lastIdxs)
       minIdx = I(1);
 else
       minIdx = I(2);
 end

end

