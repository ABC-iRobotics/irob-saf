<!-- Launch file to move on a preprogrammed trajectory -->

<launch>
	<group ns="ias">
	
	
  		<arg name="arm_typ" default="PSM1"/>
		<arg name="arm_name" default="arm_1"/>
		<arg name="camera_registration_file" 
		default="$(find irob_robot_interface)/config/registration_identity.cfg"/>
	
			<include 
			file="$(find irob_robot_interface)/config/dvrk_topic_names.xml" /> 
  	
			<node name="dvrk_interface_$(arg arm_typ)" pkg="irob_robot_interface" 
			type="irob_dvrk_interface" output="screen">
  		
  				<param name="arm_typ" type="string" value="$(arg arm_typ)" />
				<param name="arm_name" type="string" value="$(arg arm_name)" />
				<param name="camera_registration_file" type="string" 	
									value="$(arg camera_registration_file)" />
				
				
			</node>
  	</group>
</launch>
